{
  "manifest_version": 2,
  "name": "link Grabber",
  "version": "1.0.1",
  "permissions": ["activeTab", "tabs"],
  "browser_action": {
    "default_title": "Links ?",
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-3.3.1.min.js", "script.js"]
    }
  ]

}
